{
    "singleQuote": false,
    "tabWidth": 4,
    "useTabs": false,
    "trailingComma": "none",
    "semi": true,
    "jsxSingleQuote": false,
    "arrowParens": "always",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "printWidth": 120,
    "endOfLine": "lf",
    "quoteProps": "as-needed",
    "proseWrap": "preserve",
    "htmlWhitespaceSensitivity": "css",
    "embeddedLanguageFormatting": "auto"
}
